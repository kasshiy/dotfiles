[user]
  name = chickbone
  email = kasshi.4259.b.b@gmail.com
[alias]
  st = status
  co = commit
  br = branch
[core]
  editor = vim
  excludesfile = ~/.gitignore_global
  pager = "delta --dark --plus-color=\"#012800\" --minus-color=\"#340001\" --theme=\"DarkNeon\""
[url "github:"]
  InsteadOf = https://github.com/
  InsteadOf = git@github.com:
[fetch]
  prune = true
[init]
  defaultBranch = main
[pull]
  rebase = false
[merge]
  conflictstyle = diff3
  tool = vimdiff
[diff]
  colorMoved = default

[ghq]
  root = ~/.ghq
  user = "chickbone"
[delta]
  navigate = true 
